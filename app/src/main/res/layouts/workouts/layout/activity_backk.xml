<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#2a2d30"
    android:orientation="vertical"
    tools:context=".Workouts.backk">
    <include layout="@layout/backandshoulderworkout_toolbar"/>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1.0"
        android:background="?android:attr/selectableItemBackground"
        android:orientation="vertical"
        tools:ignore="InefficientWeight">

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:scrollbars="none"
            tools:ignore="NestedWeights,UselessParent">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                tools:ignore="ScrollViewSize">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="45dp">
                        <TextView
                            android:layout_marginTop="14dp"
                            android:layout_marginLeft="14dp"
                            android:textSize="19sp"
                            android:textStyle="bold"
                            android:textColor="#ffff"
                            android:fontFamily="sans-serif-condensed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_10_workouts"
                            tools:ignore="RtlHardcoded" />
                    </LinearLayout>
                </LinearLayout>

                <!-- PIKE PUSH UP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">




                    <LinearLayout
                        android:id="@+id/pikepushup1_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10.0dp"
                        android:background="@drawable/background"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/pike_push_up"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x10_repitition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/pikepushup"
                                tools:ignore="ImageContrastCheck" />

                        </LinearLayout>



                    </LinearLayout>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- TRICEPS DIP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">




                    <LinearLayout
                        android:id="@+id/tricepdipbs2_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10.0dp"
                        android:background="@drawable/background"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/tricep_dip"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x10_repitition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/tricepdip"
                                tools:ignore="ImageContrastCheck" />

                        </LinearLayout>



                    </LinearLayout>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- PUSH-UP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/pushupbs3_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:layout_margin="10.0dp"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/push_up_back_shoulder"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x12_repitition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />


                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/pushup" />

                        </LinearLayout>



                    </LinearLayout>


                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- INCLINED PUHS UP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
