                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/mountain_climbers"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x15_repetition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/mountainclimber"
                                tools:ignore="ImageContrastCheck" />

                        </LinearLayout>



                    </LinearLayout>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- PUSH BACK PUSH UP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/pushback_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:layout_margin="10.0dp"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/push_back_push_up"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x15_repetition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />


                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/pushbackpushup"
                                tools:ignore="ImageContrastCheck" />

                        </LinearLayout>



                    </LinearLayout>


                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- PLANK TO DOWN DOG -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/plankDown_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:layout_margin="10.0dp"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/plank_to_down_dog"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x20_repetition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />



                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/planktodowndog" />

                        </LinearLayout>



                    </LinearLayout>


                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- PLANK UP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/plankUP_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10.0dp"
                        android:background="@drawable/background"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment,TooManyViews">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/plank_up"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x20_repetition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/plankpushup" />

                        </LinearLayout>


                    </LinearLayout>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- ELEVATED PIKE PUSH UP -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/Elevated_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:layout_margin="10.0dp"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment,TooManyViews">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/elevated_pike_push_u"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x10_repitition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage,TextSizeCheck" />

                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/elevated_pike_push_up" />

                        </LinearLayout>


                    </LinearLayout>


                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="1.0dp"
                        tools:ignore="TooManyViews,UselessLeaf">

                    </TableRow>

                </LinearLayout>

                <!-- FLOOR Y RAISES -->
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="150.0dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/FloorY_pose"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:layout_margin="10.0dp"
                        android:orientation="horizontal"
                        tools:ignore="DisableBaselineAlignment">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10.0dp"
                            android:layout_weight="1.0"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif-condensed"
                                android:text="@string/floor_y_raises"
                                android:textAllCaps="true"
                                android:textColor="#cea86c"
                                android:textSize="19.0dp"
                                android:textStyle="bold"
                                tools:ignore="SpUsage" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/x10_repitition"
                                android:textColor="#ffffff"
                                android:textSize="15.0dp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded,SpUsage,TextSizeCheck" />

                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <pl.droidsonroids.gif.GifImageView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:src="@drawable/flooryraises" />

